{
  "name": "@fms-cat/experimental",
  "description": "Experimental edition of FMS_Cat",
  "author": "FMS_Cat",
  "version": "0.1.3",
  "main": "dist/fms-cat-experimental.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE"
  ],
  "scripts": {
    "dev": "webpack-dev-server --hot --mode development",
    "all": "yarn lint && yarn test && yarn clean && yarn build && yarn docs",
    "clean": "rimraf dist docs types",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "build": "yarn build-dev && yarn build-prod && yarn build-types",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "build-types": "tsc --emitDeclarationOnly",
    "docs": "typedoc --out docs --mode file --excludeNotExported",
    "gh-pages": "node ./utils/gh-pages.js"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "fork-ts-checker-webpack-plugin": "^4.0.0-beta.4",
    "gh-pages": "^2.1.1",
    "jest": "^25.1.0",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
