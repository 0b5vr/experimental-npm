{
  "name": "@0b5vr/experimental",
  "description": "Experimental edition of 0b5vr",
  "author": "0b5vr",
  "version": "0.7.1",
  "main": "dist/0b5vr-experimental.js",
  "module": "dist/0b5vr-experimental.module.js",
  "types": "types/index.d.ts",
  "typesVersions": {
    "<3.8": {
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "license": "MIT",
  "files": [
    "/dist/",
    "/ts*/",
    "/types/",
    "/LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "version": "yarn all",
    "dev": "cross-env DEV=1 SERVE=1 rollup -w -c -o dist/0b5vr-experimental.js",
    "all": "yarn lint && yarn test && yarn clean && yarn build && yarn docs",
    "clean": "rimraf dist/ docs/ ts*/ types/",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "build": "yarn build-dev && yarn build-prod && yarn build-esm && yarn build-esmprod && yarn build-types",
    "build-dev": "cross-env DEV=1 rollup -c -o dist/0b5vr-experimental.js",
    "build-prod": "rollup -c -o dist/0b5vr-experimental.min.js",
    "build-esm": "cross-env DEV=1 ESM=1 rollup -c -o dist/0b5vr-experimental.module.js",
    "build-esmprod": "cross-env ESM=1 rollup -c -o dist/0b5vr-experimental.module.min.js",
    "build-types": "tsc --declaration --declarationDir ./types --emitDeclarationOnly && downlevel-dts types ts3.4/types",
    "docs": "typedoc --entryPoints ./src/index.ts --out docs",
    "gh-pages": "node ./utils/gh-pages.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.2",
    "downlevel-dts": "^0.5.0",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.0.6",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "gh-pages": "^2.1.1",
    "jest": "^25.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.17.1",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "volta": {
    "node": "16.11.1",
    "yarn": "1.22.15"
  }
}
